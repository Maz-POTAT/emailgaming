<%- include('./includes/head.ejs') %>
<div class="row">
     <div class="col-md-offset-3 col-md-6 col-sm-offset-3 col-sm-6 col-xs-12">
          <div class="panel panel-info">
               <div class="panel-heading">
               Profile
               </div>
               <div class="panel-body">
                    <form role="form">
                         <div class="form-group">
                              <label>Enter Email</label>
                              <input id="user_email" class="form-control" type="text" />
                              <div id="alert_area_user_email">
                              </div>
                         </div>
                         <div class="form-group">
                              <label>Enter Password</label>
                              <input id="user_password" class="form-control" type="password" />
                              <div id="alert_area_user_password">
                              </div>
                         </div>
                         <button id="login" type="button" class="btn btn-info">Log In Now </button>
                         <button id="send_password" type="button" class="btn btn-warning">Send My Password </button>
                         <div id="alert_area_login_failed">
                         </div>
                    </form>
               </div>
          </div>
     </div>
</div>
<script src="assets/js/profile.js"></script>
<%- include('./includes/end.ejs') %>